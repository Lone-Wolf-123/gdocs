{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"dependsOn": ["@gdocs/shared#build", "^dev"],
			"cache": false,
			"persistent": true
		}
	}
}
